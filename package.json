{
  "name": "olx",
  "version": "1.0.0",
  "description": "olx.ua parser and notifier",
  "main": "index.js",
  "engines": {
    "node": "9.11.1",
    "npm": "6.0.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skip-babel": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "node dist/src/server.js",
    "start:debug": "nodemon --watch 'dist/' --inspect-brk build/dist/index.js",
    "test": "jest --watch -o src --colors",
    "test:debug": "nodemon --watch 'build/dist' --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "postinstall": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tup1tsa/olx.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tup1tsa/olx/issues"
  },
  "homepage": "https://github.com/tup1tsa/olx#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cheerio": "^0.22.8",
    "@types/compression": "0.0.36",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.2",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jest": "^23.4.1",
    "moment": "^2.22.2",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "prettier": "^1.13.7",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0"
  }
}
